    "jscalc": "0.0.4",
    "mcx-ui": "^1.1.2",
    "os": "^0.1.1",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
