{
  "name": "vueinit",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "匡淼龙 <kuangmiaolong@mujinnong.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dll": "cross-env webpack -p --config build/webpack.config.dll.js --progress --profile --colors",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config build/webpack.config.dev.js --open --hot --inline --host 0.0.0.0",
    "beta": "cross-env NODE_ENV=beta webpack --progress --config build/webpack.config.prod.js --hide-modules",
    "test": "cross-env NODE_ENV=test webpack --progress --config build/webpack.config.prod.js --hide-modules",
    "prod": "cross-env NODE_ENV=prod webpack --progress --config build/webpack.config.prod.js --hide-modules",
    "html": "cross-env python main.py",
    "lint": "eslint --ext .js --ext .vue src/",
    "lint-fix": "eslint --fix --ext .js --ext .vue src/"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "dependencies": {
    "axios": "^0.17.1",
    "fastclick": "^1.0.6",
    "font-awesome": "^4.7.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
